        "@trpc/server": "11.2.0",
        "argon2": "^0.43.0",
        "chalk": "^5.4.1",
        "cors": "2.8.5",
        "dotenv": "17.0.1",
        "express": "5.1.0",
        "fs-extra": "^11.3.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "9.0.2",
