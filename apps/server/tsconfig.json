    "compilerOptions": {
        "module": "esnext",
        "lib": ["esnext"],
        "target": "esnext",
        "esModuleInterop": true,
        "strict": true,
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true
