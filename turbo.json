{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "check": {
            "dependsOn": ["^check"]
        },
        "test": {
            "dependsOn": ["^test"]
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        }
    }
}
